---
layout: none
permalink: api/events
---
[
{% for event in site.events %}
    {
      {% for var in event %}
        {% if var == 'layout' or var == 'url' or var == 'output' or var == 'title' or var == 'slug' or var == 'date' %}
          "{{ var }}": {{ event[var] | jsonify }}{% unless var == 'date'  %},{% endunless %}
        {% endif %}
      {% endfor %}
    }{% unless forloop.last %},{% endunless %}

{% endfor %}
]