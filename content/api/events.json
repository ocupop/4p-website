---
layout: none
permalink: api/events
---
[
{% for event in site.events %}

    {
      {% for var in event %}
        {% unless var == 'next' or var == 'content' or var == 'previous' or var == 'excerpt' %}
          "{{ var }}": {{ event[var] | jsonify }}{% unless forloop.last %},{% endunless %}
        {% endunless %}
      {% endfor %}
    }{% unless forloop.last %},{% endunless %}

{% endfor %}
]