---
layout: none
permalink: api/farmers
---
[
  {% for farmer in site.farmers %}
    {
      {% for var in farmer %}
        {% if var == 'layout' or var == 'url' or var == 'output' or var == 'title' or var == 'slug' or var == 'date' or var == 'vendorID' %}
          "{{ var }}": {{ farmer[var] | jsonify }}{% unless var == 'date'  %},{% endunless %}
        {% endif %}
      {% endfor %}
    }{% unless forloop.last %},{% endunless %}
  {% endfor %}
]