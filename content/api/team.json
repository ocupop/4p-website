---
layout: none
permalink: api/team
---
[
{% for team in site.team_members %}
    {
      {% for var in team %}
        {% if var == 'layout' or var == 'url' or var == 'output' or var == 'title' or var == 'slug' or var == 'date' or var == 'vendorID' %}
          "{{ var }}": {{ team[var] | jsonify }}{% unless forloop.last %},{% endunless %}
        {% endif %}
      {% endfor %}
    }{% unless forloop.last %},{% endunless %}

{% endfor %}
]